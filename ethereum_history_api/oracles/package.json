{
  "name": "noir-ethereum-api-oracles",
  "version": "0.1.0",
  "main": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "type": "module",
  "scripts": {
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:unit:coverage": "vitest run --config vitest.unit.config.ts --coverage && echo '\nRun `open coverage/index.html` to open HTML coverage report\n'",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "format:ci": "prettier --check src",
    "generate-vk": "tsx src/script/generateVK",
    "generate-proof": "tsx src/script/generateProof",
    "oracle-server": "tsx src/script/startOracleServer",
    "oracle-server:watch": "tsx watch src/script/startOracleServer",
    "prepare-fixtures": "yarn prepare-js-fixtures && yarn prepare-noir-fixtures",
    "prepare-js-fixtures": "tsx src/script/prepareJSHistoryAPIFixtures",
    "prepare-noir-fixtures": "rm -rf ../circuits/lib/src/fixtures && tsx src/script/prepareNoirHistoryAPIFixtures && tsx src/script/prepareMerkleProofFixtures"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ethereumjs/trie": "^6.2.0",
    "@iarna/toml": "^2.2.5",
    "@noir-lang/backend_barretenberg": "0.29.0-a5b7df1.nightly",
    "@noir-lang/noir_js": "0.29.0-a5b7df1.nightly",
    "@noir-lang/noirc_abi": "0.29.0-a5b7df1.nightly",
    "dotenv": "^16.4.5",
    "execa": "^9.0.2",
    "fastify": "^4.26.2",
    "json-bigint": "^1.0.0",
    "json-rpc-2.0": "^1.7.0",
    "lodash.isequal": "^4.5.0",
    "viem": "^2.10.1"
  },
  "devDependencies": {
    "@types/iarna__toml": "^2.0.5",
    "@types/json-bigint": "^1.0.4",
    "@types/lodash.isequal": "^4.5.8",
    "@types/node": "^20.12.10",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
